<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"  "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.igalaxy.boot.mapper.sys.SysPermissionMapper">

    <select id="queryById" parameterType="long" resultType="SysPermission">
        select * from tb_sys_permission t
        where t.id = #{id} AND t.disabled = 0
    </select>
</mapper>